{
     "manifest_version": 2,
     "name": "Ad Blocker",
     "description": "Free Ad Blocker Service in websites for everyone",
     "version": "1",
     "author": "Ashwani Kumar",
     "browser_action": {
          "default_title" : "Ad Blocker"
     },
     "permissions": [
          "webRequest",
          "webRequestBlocking",
          "<all_urls>"
     ],
     "background": {
          "scripts": [
               "background.js"
          ]
     }

}